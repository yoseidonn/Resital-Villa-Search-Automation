}

/* Optional: Style when QDateEdit is focused */
QDateEdit:focus {
    border-color: #85C1E9;
}



QSpinBox {
    /* Set basic properties */
    font-size: 14px;
    padding: 8px;
    border: 1px solid #ccc;
    border-radius: 4px;
}

/* Optional: Style QSpinBox arrows */
QSpinBox::up-button, QSpinBox::down-button {
    width: 16px;
    height: 16px;
}

/* Optional: Style when QSpinBox is focused */
QSpinBox:focus {
    border-color: #85C1E9;
}



QPushButton {
    /* Set basic properties */
    font-size: 14px;
    padding: 8px 16px;
    border: 1px solid #3498db;
    border-radius: 4px;
    background-color: #3498db;
    color: #fff;
}

QPushButton:hover {
    background-color: #2980b9;
    border-color: #2980b9;
}

QPushButton:pressed {
    background-color: #1f618d;
    border-color: #1f618d;
}

/* Optional: Style when QPushButton is disabled */
QPushButton:disabled {
    background-color: #bdc3c7;
    border-color: #bdc3c7;
    color: #7f8c8d;
}



QListWidget {
    /* Set basic properties */
    font-size: 14px;
    border: 1px solid #ccc;
    border-radius: 4px;
}

QListWidget::item {
    /* Style each item in the QListWidget */
    padding: 8px;
    font-weight: bold; /* Make the item text bold */
}

QListWidget::item:selected {
    /* Style the selected item */
    background-color: #3498db;
    color: #fff;
}

/* Optional: Style when QListWidget is disabled */
QListWidget:disabled {
    background-color: #ecf0f1;
    color: #7f8c8d;
}



QGroupBox {
    /* Set basic properties */
    font-size: 16px; /* Adjust font size as needed */
    font-weight: bold; /* Make the group box title bold */
    border: 2px solid #3498db; /* Border color and width */
