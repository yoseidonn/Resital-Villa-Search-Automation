    margin-top: 10px; /* Adjust margin as needed */
    padding: 10px; /* Padding inside the group box */
    background-color: #f0f0f0; /* Background color */
}

QGroupBox::title {
    /* Style the group box title */
    subcontrol-origin: margin;
    subcontrol-position: top center; /* Position of the title */
    padding: 4px 6px; /* Padding around the title */
    background-color: #3498db; /* Title background color */
    color: white; /* Title text color */
    border-radius: 10px; /* Rounded corners */
    
}

