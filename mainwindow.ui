    font-size: 12px;
    color: #666;
    padding-bottom: 4px; /* Space between label and input */
}

/* Optional: Style when QLineEdit is focused */
QLineEdit:focus {
    border-color: #85C1E9; /* Example focus color */
}


QDateEdit {
    /* Set basic properties */
    font-size: 14px;
